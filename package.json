{
  "name": "pokedex",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://comokj7.github.io/suikadex/",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
