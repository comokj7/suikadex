    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^1.6.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "graphql.macro": "^1.4.2",
